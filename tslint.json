{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "./lib/lint-rules"
  ],
  "rules": {
    "align": [
      true,
      "parameters",
      "statements"
    ],
    "array-type": [
      true,
      "array-simple"
    ],
    "comment-format": [
      true,
      "check-space"
    ],
    "file-header": [
      true,
      "Copyright \\(C\\)",
      "Mathis Gilliot and Maxence Gilliot. All rights reserved."
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "interface-over-type-literal": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "member-ordering": [
      true,
      {
        "order": [
          "public-static-field",
          "private-static-field",
          "public-instance-field",
          "private-instance-field",
          "public-constructor",
          "private-constructor",
          "public-instance-method",
          "protected-instance-method",
          "private-instance-method"
        ]
      }
    ],
    "no-any": true,
    "no-autobind": true,
    "no-const-enum": true,
    "no-consecutive-blank-lines": true,
    "no-console": [
      true,
      "log",
      "error",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-explicit-node-module-imports": true,
    "no-relative-package-imports": true,
    "no-require-imports": true,
    "no-root-fabric-imports": true,
    "no-shadowed-variable": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": false,
    "no-unnecessary-class": true,
    "no-unused-expression": true,
    "object-literal-sort-keys": false,
    "prefer-const": [
      true,
      {
        "destructuring": "all"
      }
    ],
    "prefer-for-of": false,
    "quotemark": [
      false
    ],
    "switch-default": true,
    "typedef": [
      true,
      "parameter",
      "property-declaration"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-module",
      "check-operator",
      "check-separator",
      "check-type",
      "check-typecast"
    ]
  }
}